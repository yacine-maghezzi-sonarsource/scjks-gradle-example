node {
  stage('SCM') {
    git 'https://github.com/yacine-maghezzi-sonarsource/scjks-gradle-example.git'
  }
  stage('SonarQube analysis') {
    withSonarQubeEnv(installationName: 'SonarCloud') { // Will pick the global server connection you have configured
      sh './gradlew sonarqube -Dsonar.organization=yacine-maghezzi-sonarsource -Dsonar.projectkey=yacine-maghezzi-sonarsource_scjks-gradle-example'
    }
  }
}
