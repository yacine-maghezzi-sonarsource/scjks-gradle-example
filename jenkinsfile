pipeline {
  agent any
  stages {
    stage('SonarCloud analysis') {
      steps {
        withSonarQubeEnv(installationName: 'SonarCloud') {
          script {
            sh 'JAVA_HOME="/usr/local/opt/openjdk@11/" ./gradlew sonarqube -Dsonar.organization=yacine-maghezzi-sonarsource -Dsonar.projectkey=yacine-maghezzi-sonarsource_scjks-gradle-example -S'
          }
        }
      }
    }
  }
}
