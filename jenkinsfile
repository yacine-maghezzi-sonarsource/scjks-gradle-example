pipeline {
  agent any
  stages {
    stage('SonarCloud analysis') {
      steps {
        withSonarQubeEnv(installationName: 'SonarCloud') {
          script {
            sh 'echo $JAVA_HOME'
            export JAVA_HOME="/usr/local/opt/openjdk@11/"
            sh 'echo $JAVA_HOME'
            echo $JAVA_HOME
            sh './gradlew sonarqube -Dsonar.organization=yacine-maghezzi-sonarsource -Dsonar.projectkey=yacine-maghezzi-sonarsource_scjks-gradle-example'
          }
        }
      }
    }
  }
}
