pipeline {
  agent any
  stages {
    stage('SonarCloud analysis') {
      steps {
        withSonarQubeEnv(installationName: 'SonarCloud') {
          script {
            def $JAVA_HOME = tool 'JDK11';
            sh 'export JAVA_HOME="/usr/local/opt/openjdk@11/"'
            sh 'echo $JAVA_HOME'
            sh './gradlew --status'
            sh 'java --version'
            sh './gradlew sonarqube -Dsonar.organization=yacine-maghezzi-sonarsource -Dsonar.projectkey=yacine-maghezzi-sonarsource_scjks-gradle-example -S'
          }
        }
      }
    }
  }
}
